<?xml version="1.0"?>
<simulation xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:noNamespaceSchemaLocation="../src/xsd/Simulation.xsd">
    <simulationParams>
        <endTime>15</endTime>
        <deltaT> 0.0005</deltaT>
        <modelType>lennardJones</modelType>
        <containerType>linkedCells</containerType>
        <plotInterval>100</plotInterval>
        <objectType>cuboid</objectType>
        <checkpointing>false</checkpointing>
    </simulationParams>

    <linkedCellParams>
        <domainSize>
            <Lx>303</Lx>
            <Ly>180</Ly>
            <Lz>1</Lz>
        </domainSize>
        <cutoffRadius>3</cutoffRadius>
        <boundaryConditions>
            <left>p</left>
            <up>r</up>
            <right>p</right>
            <down>r</down>
            <front>p</front>
            <back>r</back>
        </boundaryConditions>
        <gravitationalAcceleration>-12.44</gravitationalAcceleration>
        <isMembrane>true</isMembrane>
    </linkedCellParams>

    <membraneParams>
        <stiffness>300</stiffness>
        <averageBond>2.2</averageBond>
        <pullUpForce>0.8</pullUpForce>
    </membraneParams>
</simulation>